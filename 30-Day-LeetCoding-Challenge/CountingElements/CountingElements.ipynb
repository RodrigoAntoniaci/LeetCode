    "class Solution(object):\n",
    "    def countElements(self, arr):\n",
    "        set_arr = set(arr)\n",
    "        count = 0\n",
    "        \n",
    "        for i in arr:\n",
    "            if i+1 in set_arr:\n",
    "                count += 1\n",
    "            \n",
    "        return count"
   ]
  }
