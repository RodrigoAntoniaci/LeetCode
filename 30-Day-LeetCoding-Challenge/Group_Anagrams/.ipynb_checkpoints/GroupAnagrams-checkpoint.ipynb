    "class Solution(object):\n",
    "    def groupAnagrams(self, strs): \n",
    "        answer = collections.defaultdict(list)\n",
    "        \n",
    "        for i in strs:\n",
    "        \n",
    "            answer[''.join((sorted(i)))].append(i)       \n",
    "        \n",
    "        return answer.values()"
   ]
  }
