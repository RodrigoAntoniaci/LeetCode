   "source": [
    "class Solution(object):\n",
    "    def maxSubArray(self, nums):\n",
    "        ans = float('-inf')\n",
    "        n_sum = 0\n",
    "        for n in nums:\n",
    "            n_sum = max(n,n_sum + n)\n",
    "            ans = max(ans, n_sum)\n",
    "        return ans"
   ]
  }
