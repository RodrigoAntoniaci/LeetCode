   "source": [
    "class Solution:\n",
    "    def checkValidString(self,s):\n",
    "        lp = rp = 0\n",
    "        for ch in s:\n",
    "            lp += 1 if ch == '(' else -1\n",
    "            rp += 1 if ch != ')' else -1\n",
    "            if rp < 0: break\n",
    "            lp = max(lp,0)\n",
    "        return lp == 0"
   ]
  }
